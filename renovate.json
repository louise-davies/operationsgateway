{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    "schedule:nonOfficeHours",
    ":maintainLockFilesWeekly",
    "npm:unpublishSafe"
  ],
  "postUpdateOptions": ["yarnDedupeHighest"],
  "labels": ["dependencies"],
  "separateMinorPatch": true,
  "patch": {
    "enabled": false
  },
  "cdnurl": {
    "fileMatch": ["^plotWindowPortal\\.component\\.tsx$"]
  }
}
